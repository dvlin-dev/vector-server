{
  "name": "vector-server",
  "version": "0.0.1",
  "description": "vector server",
  "author": "dvlin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "start:web-search": "ts-node src/web-search.main.ts",
    "start:web-search:dev": "cross-env NODE_ENV=development nest start --watch --config nest-web-search.json",
    "start:web-search:prod": "cross-env NODE_ENV=production node dist/src/web-search.main",
    "build:web-search": "cross-env NODE_ENV=production nest build --config nest-web-search.json",
    "start:oss": "ts-node src/oss.main.ts",
    "start:oss:dev": "cross-env NODE_ENV=development nest start --watch --config nest-oss.json",
    "start:oss:prod": "cross-env NODE_ENV=production node dist/src/oss.main",
    "build:oss": "cross-env NODE_ENV=production nest build --config nest-oss.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:generate:vector": "prisma generate --schema=prisma/vector.schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:migrate:vector": "prisma migrate dev --name=vector_init --create-only --schema=prisma/vector.schema.prisma",
    "prisma:db:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:db:push:vector": "prisma db push --schema=prisma/vector.schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/mapped-types": "0.2.0",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "^5.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "gpt3-tokenizer": "^1.1.5",
    "helmet": "^6.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.0.163",
    "module-alias": "^2.2.2",
    "nest-winston": "^1.10.2",
    "node-fetch": "^2.6.1",
    "openai": "^5.12.2",
    "path": "^0.12.7",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sharp": "^0.34.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "28.1.8",
    "@types/node": "^16.18.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
